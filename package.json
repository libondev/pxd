{
  "name": "carbons",
  "version": "0.0.1",
  "type": "module",
  "description": "A simple component library for Vue@3.",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "index.d.ts",
  "files": ["dist/*", "types/*"],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./*": {
      "import": "./dist/*",
      "require": "./dist/*"
    }
  },
  "scripts": {
    "gen": "zx ./scripts/gc.mjs --quiet",
    "dev": "cd ./playground && npm run dev",
    "build": "node ./scripts/builds/index.js",
    "lint": "eslint ./src --ext .vue,.ts,.tsx --fix --ignore-path .gitignore"
  },
  "keywords": [],
  "author": "banlify <banlify@163.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/banlify/carbons.git"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.9",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.38.0",
    "esbuild": "^0.15.9",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "rollup": "^2.79.1",
    "rollup-plugin-esbuild": "^4.10.1",
    "rollup-plugin-sass": "^1.12.16",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.8.3",
    "vue": "^3.2.39",
    "zx": "^7.0.8"
  }
}
