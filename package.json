{
  "name": "carbons",
  "version": "0.0.1",
  "type": "module",
  "description": "A simple component library for Vue@3.",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": [
    "dist/*"
  ],
  "files": [
    "dist/*",
    "types/*"
  ],
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "scripts": {
    "gen": "zx ./scripts/gc.mjs",
    "dev": "cd ./playground && npm run dev",
    "build": "npm run build:lib && npm run build:styles",
    "build:lib": "node ./scripts/build-libs.mjs",
    "build:styles": "node ./scripts/build-styles.mjs",
    "stylelint": "stylelint --fix src/**/*.{scss,css}",
    "lint": "eslint ./src --ext .vue,.ts,.tsx --fix --ignore-path .gitignore"
  },
  "keywords": [],
  "author": "banlify <banlify@163.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/banlify/carbons.git"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "csso": "^5.0.5",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "kolorist": "^1.5.1",
    "magic-string": "^0.26.3",
    "rollup": "^2.79.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.10.1",
    "stylelint": "^14.9.1",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-scss": "^4.2.0",
    "typescript": "^4.8.3",
    "vite": "^3.1.2",
    "vite-plugin-dts": "^1.1.1",
    "vue": "^3.2.39",
    "zx": "^7.0.8"
  }
}
