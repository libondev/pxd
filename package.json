{
  "name": "pxd",
  "type": "module",
  "version": "0.0.3",
  "description": "Vue component library based on radix-vue and geist design system.",
  "author": "libondev <bon.li@outlook.com>",
  "license": "MIT",
  "homepage": "https://github.com/libondev/pxd#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/libondev/pxd.git"
  },
  "bugs": {
    "url": "https://github.com/libondev/pxd/issues"
  },
  "keywords": [
    "ui",
    "vue",
    "vue3",
    "ui framework",
    "component library"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./vars.css": "./dist/vars.css",
    "./resolver": "./dist/plugins/resolver.js",
    "./*": "./dist/*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "style": "dist/vars.css",
  "files": [
    "dist/*",
    "res/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite --config vite.config.dev.ts",
    "build": "vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "vue-tsc --noEmit --composite false"
  },
  "peerDependencies": {
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@iconify-json/ic": "^1.2.1",
    "@types/node": "^20.16.12",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.12.0",
    "fast-glob": "^3.3.2",
    "postcss": "^8.4.47",
    "radix-vue": "^1.9.7",
    "tailwindcss": "^3.4.14",
    "typescript": "~5.6.3",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.9",
    "vite-plugin-dts": "^4.2.4",
    "vue": "^3.5.12",
    "vue-tsc": "^2.1.6"
  }
}
