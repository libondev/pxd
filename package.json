{
  "name": "pxd",
  "type": "module",
  "version": "0.0.2",
  "description": "A library of Vue3 components based on radix-vue and unocss.",
  "author": "libondev <bon.li@outlook.com>",
  "license": "MIT",
  "homepage": "https://github.com/libondev/pxd#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/libondev/pxd.git"
  },
  "bugs": {
    "url": "https://github.com/libondev/pxd/issues"
  },
  "keywords": [
    "ui",
    "vue",
    "vue3",
    "ui framework",
    "component library"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.js",
    "./styles.css": "./dist/styles.css",
    "./resolver": "./dist/plugins/resolver.js",
    "./*": "./dist/*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "style": "dist/styles.css",
  "files": [
    "dist/*",
    "res/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "type-check": "vue-tsc --noEmit --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "dependencies": {
    "radix-vue": "^1.8.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.18.1",
    "@iconify-json/ic": "^1.1.17",
    "@types/node": "^20.12.12",
    "@unocss/transformer-variant-group": "^0.60.3",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "animated-unocss": "^0.0.6",
    "eslint": "^9.3.0",
    "fast-glob": "^3.3.2",
    "lightningcss": "^1.25.0",
    "radix-vue": "^1.8.0",
    "typescript": "~5.4.5",
    "unocss": "^0.60.3",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^5.2.11",
    "vite-plugin-dts": "^3.9.1",
    "vue": "^3.4.27",
    "vue-tsc": "^2.0.19"
  }
}
