{
  "name": "pxd",
  "type": "module",
  "version": "0.0.1",
  "description": "A stylish modern component library for Vue3. 一个别具一格的现代 Vue3 组件库。",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "style": "dist/_styles/index.css",
  "sideEffects": ["dist/*", "dist/_styles/*"],
  "files": ["dist/*"],
  "exports": {
    ".": "./dist/index.js",
    "./index.css": "./dist/_styles/index.css",
    "./*": "./dist/*"
  },
  "scripts": {
    "gen": "zx ./scripts/gc.mjs",
    "dev": "cd ./playground && npm run dev",
    "build": "npm run build:lib && npm run build:styles",
    "build:lib": "vite build -c ./scripts/build/library.mjs",
    "build:styles": "node ./scripts/build/styles.mjs",
    "stylelint": "stylelint --fix src/**/*.{css,scss}",
    "eslint": "eslint ./src --ext .vue,.ts,.tsx --fix --ignore-path .gitignore"
  },
  "keywords": [
    "ui",
    "vue",
    "vue3",
    "ui framework",
    "component library"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "author": "banlify <banlify@163.com>",
  "homepage": "https://github.com/banlify/pxd#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/banlify/pxd.git"
  },
  "bugs": {
    "url": "https://github.com/banlify/pxd/issues"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@vitejs/plugin-vue": "^3.1.2",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "@vue/eslint-config-typescript": "^11.0.2",
    "csso": "^5.0.5",
    "eslint": "^8.25.0",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.3.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unicorn": "^43.0.2",
    "eslint-plugin-vue": "^9.6.0",
    "fast-glob": "^3.2.12",
    "fs-extra": "^10.1.0",
    "kolorist": "^1.6.0",
    "magic-string": "^0.26.7",
    "sass": "^1.55.0",
    "stylelint": "^14.14.0",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "^4.8.4",
    "unplugin-auto-import": "^0.11.2",
    "unplugin-vue-components": "^0.22.8",
    "unplugin-vue-setup-extend-plus": "^0.3.2",
    "vite": "^3.1.8",
    "vite-plugin-dts": "^1.6.6",
    "vue": "^3.2.41",
    "zx": "^7.1.1"
  },
  "dependencies": {
    "@vueuse/core": "^9.3.1",
    "focus-trap": "^7.0.0"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ]
}
